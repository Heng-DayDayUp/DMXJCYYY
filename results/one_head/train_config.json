{
  "task": "seq2seq",
  "data": "data/tiny_shakespeare.txt",
  "seq_len": 128,
  "batch_size": 32,
  "d_model": 128,
  "d_ff": 512,
  "heads": 1,
  "layers": 2,
  "dropout": 0.1,
  "lr": 0.0003,
  "weight_decay": 0.01,
  "warmup": 1000,
  "grad_clip": 1.0,
  "epochs": 6,
  "seed": 42,
  "log_interval": 100,
  "save": "results/one_head",
  "max_len": 5000,
  "use_pos_encoding": true,
  "relative_pos": false,
  "max_rel": 128,
  "no_residual": false
}